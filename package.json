{
  "name": "prepmate",
  "version": "1.0.0",
  "description": "A MERN stack placement trainer web app using Gemini API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "client": "cd client && npm start",
    "build": "npm run client:build",
    "client:build": "cd client && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "placement",
    "interview",
    "gemini",
    "ai"
  ],
  "author": "Prepmate Team",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@google/generative-ai": "^0.2.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "mammoth": "^1.11.0",
    "method-override": "^3.0.0",
    "mongoose": "^8.0.3",
    "multer": "^2.0.0-rc.4",
    "node-cron": "^4.2.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.3"
  }
}
