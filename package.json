{
  "name": "prepmate",
  "version": "1.0.0",
  "description": "A MERN stack placement trainer web app using Gemini API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": ["mern", "placement", "interview", "gemini", "ai"],
  "author": "Prepmate Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^2.0.0-rc.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "@google/generative-ai": "^0.2.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "concurrently": "^8.2.2"
  }
}
